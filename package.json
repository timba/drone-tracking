{
  "name": "drone-tracker",
  "version": "1.0.0",
  "description": "Simple server to track location telemetry from drones",
  "main": "./build/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "protoc": "protoc --js_out=import_style=commonjs,binary:. --ts_out=. --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts ./src/loc-message.proto",
    "start-server": "node ./build/server.js",
    "start-client": "node ./build/client.js"
  },
  "author": "Timur Babyuk",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.12.14",
    "@types/express": "^4.17.2",
    "@types/express-handlebars": "^3.1.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/randomstring": "^1.1.6",
    "@types/source-map-support": "^0.5.0",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "randomstring": "^1.1.5",
    "source-map-support": "^0.5.16"
  }
}
